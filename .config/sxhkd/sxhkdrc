super + Return
				alacritty
super + b
				firefox
super + f
				pcmanfm
super + r
				rofi -show run
XF86AudioRaiseVolume
				pactl set-sink-volume @DEFAULT_SINK@ +1%
XF86AudioLowerVolume
				pactl set-sink-volume @DEFAULT_SINK@ -1%
XF86AudioMute
				pactl set-sink-mute @DEFAULT_SINK@ toggle
XF86AudioPlay
#				playerctl play-pause
				~/.scripts/playerctl-custom.sh
XF86AudioNext
				playerctl next
XF86AudioPrev
				playerctl previous
super + shift + ctrl + q
				shutdown -P now
